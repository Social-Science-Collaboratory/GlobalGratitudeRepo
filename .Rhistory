# Function for skewness and kurtosis z-test
spssSkewKurtosis <- function(x) {
w <- length(x)
m1 <- mean(x)
m2 <- sum((x - m1)^2)
m3 <- sum((x - m1)^3)
m4 <- sum((x - m1)^4)
s1 <- sd(x)
skew <- w * m3 / ((w - 1) * (w - 2) * s1^3)
sdskew <- sqrt(6 * w * (w - 1) / ((w - 2) * (w + 1) * (w + 3)))
kurtosis <- (w * (w + 1) * m4 - 3 * m2^2 * (w - 1)) / ((w - 1) * (w - 2) * (w - 3) * s1^4)
sdkurtosis <- sqrt(4 * (w^2 - 1) * sdskew^2 / ((w - 3) * (w + 5)))
z.skew <- skew / sdskew
z.kurt <- kurtosis / sdkurtosis
return(data.frame(skew, kurtosis, z.skew, z.kurt))
}
# Main function to run all assessments and create combined plots
run_normality_assessments_combined <- function(df, vars) {
for (var in vars) {
cat("\n-----------------------------\n")
cat("Normality Assessment for:", var, "\n")
x <- na.omit(df[[var]])
x_df <- data.frame(value = x)
## A. Histogram with normal curve
p1 <- ggplot(x_df, aes(x = value)) +
geom_histogram(aes(y = ..density..), bins = 30, fill = "lightblue", color = "black") +
stat_function(fun = dnorm, args = list(mean = mean(x), sd = sd(x)), color = "red", size = 1.2) +
ggtitle(paste("Histogram of", var)) +
theme_minimal()
## B. Boxplot
p2 <- ggplot(x_df, aes(y = value)) +
geom_boxplot(fill = "lightgreen", width = 0.4) +
ggtitle(paste("Boxplot of", var)) +
theme_minimal()
## C. QQ Plot
p3 <- ggqqplot(x_df$value, title = paste("Q-Q Plot of", var)) +
theme_minimal()
## D. Stem-and-leaf (printed to console)
cat("\nStem-and-leaf plot:\n")
stem(x)
## E. Skewness, kurtosis, tests
print(spssSkewKurtosis(x))
print(shapiro.test(x))
print(lillie.test(x))
## Combine plots
grid.arrange(p1, p2, p3, ncol = 3)
}
}
